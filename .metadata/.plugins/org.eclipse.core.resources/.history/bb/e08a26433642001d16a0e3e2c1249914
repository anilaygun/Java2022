package methods;

public class Driver {

	public static void main(String[] args) {
		sayiBulmaca();

	}

	public static void sayiBulmaca() {
		int[] sayilar = new int[] { 1, 2, 3, 5, 7, 9 };
		int aranacak = 6;
		boolean varMi = false;

		for (int sayi : sayilar) {
			if (sayi == aranacak) {
				varMi = true;
				break;
			}
		}
	}

	public static void mesajVer(int aranacak) {
		System.out.println("SayÄ± mevcuttur: " + aranacak);
	}
}
